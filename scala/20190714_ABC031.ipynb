    "N = int(input())\n",
    "S = input()\n",
    "list_s = list(S)\n",
    "\n",
    "from collections import Counter\n",
    "\n",
    "counter = Counter(list_s)\n",
    "\n",
    "LARGE_NUM = 10**9 + 7\n",
    "\n",
    "result = 1\n",
    "for _, cnt in counter.items():\n",
    "    result *= cnt+1\n",
    "    result %= LARGE_NUM\n",
    "print(result-1)"
   ]
